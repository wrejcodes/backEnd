{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend api for csc 425",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon server.js",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js --exit",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js --exit",
    "lint": "eslint .",
    "coverage": "nyc npm run test && npm run coverage:file",
    "coverage:file": "nyc report --reporter=text-lcov  > coverage/lcov.info",
    "coveralls": "npm run coverage && nyc report --reporter=text-lcov | coveralls",
    "init:db": "npm run init:database && npm run init:tables",
    "init:database": "./node_modules/sequelize-cli/lib/sequelize db:create",
    "init:tables": "./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "init:db:test": "npm run init:database:test && npm run init:tables:test",
    "init:database:test": "./node_modules/sequelize-cli/lib/sequelize db:create --env test",
    "init:tables:test": "./node_modules/sequelize-cli/lib/sequelize db:migrate --env test",
    "parse": "ruby utils/parse_csv.rb",
    "generate:models": "ruby utils/make_models.rb",
    "recreate:dev_test": "ruby utils/recreate_db.rb",
    "generate:unit_test": "ruby utils/generate.rb unit_test",
    "generate:route": "ruby utils/generate.rb route",
    "make_app": "ruby utils/make_app.rb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gods-Of-ACM/backEnd.git"
  },
  "author": "Quintin Donnelly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gods-Of-ACM/backEnd/issues"
  },
  "homepage": "https://github.com/Gods-Of-ACM/backEnd#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "helmet": "^3.11.0",
    "jsonresponse": "^0.1.3",
    "jsonwebtoken": "^8.1.1",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.2",
    "nodemon": "^1.15.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "pug": "^2.0.0-rc.4",
    "sequelize": "^4.33.4",
    "sequelize-cli": "^4.0.0",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.0",
    "eslint-config-google": "^0.9.1",
    "mocha": "^5.0.1"
  }
}
